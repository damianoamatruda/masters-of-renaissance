<?xml version="1.0" encoding="UTF-8"?>

<?import it.polimi.ingsw.client.gui.components.SButton?>
<?import it.polimi.ingsw.client.gui.components.Title?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16"
         type="StackPane"
         stylesheets="@../index.css">
  <Pane styleClass="overlay"/>
  <BorderPane fx:id="main">
    <center>
      <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" styleClass="floating"
            spacing="50">
        <Title fx:id="titleText" maxWidth="-Infinity">
        </Title>
        <Text fx:id="contentText"/>
        <SButton fx:id="okButton" alignment="CENTER" defaultButton="true" prefWidth="150.0" text="OK">
          <padding>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0"/>
          </padding>
        </SButton>
        <padding>
          <Insets bottom="30.0" left="30.0" right="30.0" top="30.0"/>
        </padding>
      </VBox>
    </center>
  </BorderPane>
</fx:root>
